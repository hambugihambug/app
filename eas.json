{
    "cli": {
        "version": ">= 16.3.3",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "ios": {
                "simulator": true,
                "buildConfiguration": "Debug",
                "image": "latest"
            },
            "developmentClient": true,
            "distribution": "internal",
            "env": {
                "EXPO_NO_TYPESCRIPT_TRANSPILE": "1"
            }
        },
        "preview": {
            "distribution": "internal"
        },
        "production": {
            "autoIncrement": true
        }
    },
    "submit": {
        "production": {}
    }
}
